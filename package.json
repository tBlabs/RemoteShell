    },
    "scripts": {
        "xxx_preinstall": "sudo npm i -g typescript jest tslint npm-run-all",
        "start": "node ./bin/src/startup.js",
        "run": "npm run clean && npm run build && clear && npm run start",
        "watch": "tsc --watch",
        "monitor": "nodemon ./bin/src/startup.js",
